@model IEnumerable<System.Security.Claims.Claim>
@{
    ViewData["Title"] = "Index";
}

<h1>User Claims</h1>
<table class="table table-hover table-bordered">
    <tr>
        <td>Subject</td>
        <td>Issuer</td>
        <td>Type</td>
        <td>Value</td>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Subject.Name</td>
            <td>@item.Issuer</td>
            <td>@item.Type</td>
            <td>@item.Value</td>
        </tr>
    }
</table>

